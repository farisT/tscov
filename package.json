{
    "name": "tscov",
    "version": "1.3.0",
    "description": "TypeScript CLI to calculate type coverage",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "repository": "https://github.com/jeroenouw/tscov.git",
    "author": "Jeroen Ouwehand",
    "license": "MIT",
    "scripts": {
        "start": "npm run build && npm run global",
        "build": " rm -rf ./lib && tsc -p .",
        "global": "sudo npm i -g && tscov",
        "link-upstream": "git remote add upstream https://github.com/jeroenouw/tscov",
        "sync": "git fetch origin && git checkout master && git pull && git checkout <your-branch> && git merge master && git push",
        "reset": "rm -rf ./node_modules ./package-lock.json ./lib && npm install"
    },
    "typeCoverage": {
        "minCoverage": 90
    },
    "bin": {
        "tscov": "bin/tscov"
    },
    "keywords": [
        "cli",
        "tscov",
        "typescript",
        "javascript",
        "type",
        "coverage",
        "nodejs"
    ],
    "dependencies": {
        "commander": "^2.19.0",
        "figlet": "^1.2.1",
        "glob": "7.1.3",
        "inversify": "^5.0.1",
        "minimist": "1.2.0",
        "prompts": "^2.0.1",
        "reflect-metadata": "^0.1.13",
        "tslib": "1.9.3"
    },
    "devDependencies": {
        "@types/glob": "7.1.1",
        "@types/jasmine": "3.3.7",
        "@types/minimist": "1.2.0",
        "@types/node": "10.12.18",
        "jasmine": "3.3.1",
        "no-unused-export": "1.7.0",
        "standard": "12.0.1",
        "ts-node": "^7.0.1",
        "tslint": "5.12.1",
        "tslint-config-standard": "8.0.1",
        "tslint-sonarts": "1.8.0",
        "typescript": "^3.3.1"
    }
}
